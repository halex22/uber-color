{
    "source": {
      "include": ["src"], 
      "includePattern": ".js$",
      "excludePattern": "(node_modules|docs)"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "template": "templates/default"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "default": {
        "outputSourceFiles": true
      }
    }
  }
  